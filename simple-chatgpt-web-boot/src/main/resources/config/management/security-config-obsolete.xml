<?xml version="1.0" encoding="UTF-8" ?>
<security-config>

	<!-- ========================= -->
	<!-- Base Roles -->
	<!-- ========================= -->
	<roles>
		<role name="ROOT_ROLE" description="ROOT privileges" />
		<role name="ADMIN_ROLE" description="Admin privileges" />
		<role name="USER_ROLE" description="Standard user privileges" />
	</roles>

	<!-- ========================= -->
	<!-- Role Groups -->
	<!-- ========================= -->
	<role-groups>
		<!-- User Role Groups -->
		<role-group name="ROOT_USER_ROLE_GROUP"
			delimitRoles="|ROOT_ROLE|ADMIN_ROLE|USER_ROLE|">
			<role-ref name="ROOT_ROLE" />
			<role-ref name="ADMIN_ROLE" />
			<role-ref name="USER_ROLE" />
		</role-group>
		<role-group name="ADMIN_USER_ROLE_GROUP"
			delimitRoles="|ADMIN_ROLE|USER_ROLE|">
			<role-ref name="ADMIN_ROLE" />
			<role-ref name="USER_ROLE" />
		</role-group>
		<role-group name="BASIC_USER_ROLE_GROUP"
			delimitRoles="|USER_ROLE|">
			<role-ref name="USER_ROLE" />
		</role-group>

		<!-- Page Role Groups -->
		<role-group name="ADMIN_PAGE_ROLE_GROUP"
			delimitRoles="|ADMIN_ROLE|">
			<role-ref name="ADMIN_ROLE" />
		</role-group>
		<role-group name="USER_PAGE_ROLE_GROUP"
			delimitRoles="|USER_ROLE|">
			<role-ref name="USER_ROLE" />
		</role-group>
	</role-groups>

	<!-- ========================= -->
	<!-- Page → Role Group mapping -->
	<!-- ========================= -->
	<page-role-groups>
		<!-- Property -->
		<page-role-group url-pattern="/property/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/property/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />

		<!-- Role -->
		<page-role-group url-pattern="/role/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group url-pattern="/management/jsp/role/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />

		<!-- RoleGroup -->
		<page-role-group url-pattern="/roleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/roleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />

		<!-- RoleGroupRole -->
		<page-role-group url-pattern="/roleGroupRole/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/roleGroupRole/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />

		<!-- User -->
		<page-role-group url-pattern="/user/**"
			role-group-ref="USER_PAGE_ROLE_GROUP" />
		<page-role-group url-pattern="/management/jsp/user/**"
			role-group-ref="USER_PAGE_ROLE_GROUP" />

		<!-- UserList -->
		<page-role-group url-pattern="/userList/**"
			role-group-ref="USER_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/userList/**"
			role-group-ref="USER_PAGE_ROLE_GROUP" />

		<!-- UserRoleGroup -->
		<page-role-group url-pattern="/userRoleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/userRoleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />

		<!-- PageRoleGroup -->
		<page-role-group url-pattern="/pageRoleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
		<page-role-group
			url-pattern="/management/jsp/pageRoleGroup/**"
			role-group-ref="ADMIN_PAGE_ROLE_GROUP" />
	</page-role-groups>

	<!-- ========================= -->
	<!-- Default Pages -->
	<!-- ========================= -->
	<pages>
		<!-- Property -->
		<page url-pattern="/property/**"
			delimit-role-group="|ADMIN_PAGE_ROLE_GROUP|" />
		<page url-pattern="/management/jsp/property/**"
			delimit-role-group="|ADMIN_PAGE_ROLE_GROUP|" />

		<!-- Role -->
